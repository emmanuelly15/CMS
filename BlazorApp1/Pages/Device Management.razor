@page "/DeviceManagement"

<h3>Device Management</h3>

<p>Add, edit, or delete a device.</p>

<div class="card"> 
    <div class="card-body">
        <h4>Add Device</h4>


        <button class="btn btn-primary">
            <span class="oi oi-plus"></span>
            Add Device
        </button>
    </div>
</div>


<table class="table">
        <thead>
            <tr>
                <th>#</th>
                <th>IMEI</th>
                <th>User name</th>
                <th>Group</th>
                <th>Registered On</th>
                <th>Last Update</th>
                <th>Last Location</th>
                <th>Authorised</th>
                <th>Actions</th>
            </tr>
        </thead>
    </table>

@code {

}
