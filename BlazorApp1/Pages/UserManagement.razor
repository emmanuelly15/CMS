@page "/userManagement"
<h1>User Management</h1>
<h2>Add User</h2>


<div class="row">
        <div class="col-md-6">
                <div class="form-group">
                        <label for="Name">  Name *</label>
                        <input form="Name" class="form-control"  />
                    </div>
            </div>
        
        <div class="col-md-6">
                <div class="form-group">
                        <label for="Email"> Email *</label>
                        <input form="Email" class="form-control"  />
                    </div>
            </div>
        <div class="col-md-6">
                <div class="form-group">
                        <label for="Telephone"> Telephone *</label>
                        <input form="Telephone" class="form-control"  />
                    </div>
            </div>
            <div class="col-md-6">
                <div class="form-group">
                        <label for="EmpId"> Employee_ID *</label>
                        <input form="EmpId" class="form-control"  />
                    </div>
            </div>
            <div class="col-md-6">
                <div class="form-group">
                        <label for="Password"> Password *</label>
                        <input form="Password" class="form-control"  />
                    </div>
            </div>
</div>
<div class="row">
        <div class="col-md-4">
                <div class="form-group">
                        <input type="button" class="btn btn-primary"  value="Save" />
                        <input type="button" class="btn"  value="Cancel" />
                    </div>
            </div>
</div>
